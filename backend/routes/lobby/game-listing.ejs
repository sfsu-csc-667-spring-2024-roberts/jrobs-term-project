<template id="available-game-template">
  <%- include("available-game", { game: { id: -1, gravatar: "", email: "@"}}) %>
</template>
<ul
  role="list"
  id="available-games-list"
  class="mt-3 grid grid-cols-1 gap-5 sm:grid-cols-2 sm:gap-6 lg:grid-cols-2"
>
  <% availableGames.forEach(game => { %>
  <%- include("available-game", { game }) %>
  <% }) %>
</ul>
